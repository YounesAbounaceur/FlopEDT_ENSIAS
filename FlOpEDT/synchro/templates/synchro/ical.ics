BEGIN:VCALENDAR
VERSION:2.0
PRODID:FLOP-EDT V1.0
METHOD:PUBLISH
BEGIN:VTIMEZONE
TZID:{{ timezone }}
X-LIC-LOCATION:{{ timezone }}
BEGIN:DAYLIGHT
TZOFFSETFROM:+0100
TZOFFSETTO:+0200
TZNAME:CEST
DTSTART:19700329T020000
RRULE:FREQ=YEARLY;BYDAY=-1SU;BYMONTH=3
END:DAYLIGHT
BEGIN:STANDARD
TZOFFSETFROM:+0200
TZOFFSETTO:+0100
TZNAME:CET
DTSTART:19701025T030000
RRULE:FREQ=YEARLY;BYDAY=-1SU;BYMONTH=10
END:STANDARD
END:VTIMEZONE
{% for e in events %}

BEGIN:VEVENT
UID:{{ e.id }}@schedule
DTSTAMP:{% now 'Ymd\THis\Z' %}
SUMMARY:{{ e.title }}
LOCATION:{{ e.location }}
DESCRIPTION:{{ e.description }}
DTSTART;TZID={{ timezone }}:{{ e.begin|date:'Ymd\THis' }}
DTEND;TZID={{ timezone }}:{{ e.end|date:'Ymd\THis' }}
END:VEVENT
{% endfor %}
END:VCALENDAR
